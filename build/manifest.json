{
  "name": "Yumazzo-Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "This is my extension description.",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./images/icon16.png",
    "32": "./images/icon32.png",
    "48": "./images/icon48.png",
    "128": "./images/icon128.png"
  },
  "host_permissions": ["<all_urls>"]
}
